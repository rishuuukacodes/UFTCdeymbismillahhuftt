#membuat database#

CREATE DATABASE IF NOT EXISTS db_lomba_finalis;
USE db_lomba_finalis;

#tabel fakultas

CREATE TABLE Fakultas (
    id_fakultas INT AUTO_INCREMENT PRIMARY KEY,
    nama_fakultas VARCHAR(100) NOT NULL
) ENGINE=InnoDB;

#tabel prodi#

CREATE TABLE Prodi (
    id_prodi INT AUTO_INCREMENT PRIMARY KEY,
    nama_prodi VARCHAR(150) NOT NULL,
    id_fakultas INT NOT NULL,
    CONSTRAINT fk_prodi_fakultas
        FOREIGN KEY (id_fakultas)
        REFERENCES Fakultas(id_fakultas)
        ON UPDATE CASCADE
        ON DELETE RESTRICT
) ENGINE=InnoDB;

#tabel cabang lomba#

CREATE TABLE Cabang_Lomba (
    id_cabang INT AUTO_INCREMENT PRIMARY KEY,
    nama_cabang VARCHAR(100) NOT NULL
) ENGINE=InnoDB;

#tabel finalis#

CREATE TABLE Finalis (
    id_finalis INT AUTO_INCREMENT PRIMARY KEY,
    nama_finalis VARCHAR(150) NOT NULL,
    no_whatsapp VARCHAR(30),
    nim VARCHAR(30),
    id_prodi INT NOT NULL,
    id_cabang INT NOT NULL,

    CONSTRAINT fk_finalis_prodi
        FOREIGN KEY (id_prodi)
        REFERENCES Prodi(id_prodi)
        ON UPDATE CASCADE
        ON DELETE RESTRICT,

    CONSTRAINT fk_finalis_cabang
        FOREIGN KEY (id_cabang)
        REFERENCES Cabang_Lomba(id_cabang)
        ON UPDATE CASCADE
        ON DELETE RESTRICT
) ENGINE=InnoDB;

